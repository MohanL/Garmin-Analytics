{
    "name": "Garmin Analytics",
    "description": "Garmin Analytics Chrome extension",
    "version": "0.1.0",
    "manifest_version": 3,
    "permissions": ["scripting"],
    "content_scripts": [
        {
          "matches": ["https://connect.garmin.com/modern/*"],
          "js": ["background.js"]
        }
      ]
  }